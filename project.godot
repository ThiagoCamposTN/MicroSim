; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Mipro"
run/main_scene="res://Scenes/Main.tscn"
config/features=PackedStringArray("4.3")
config/icon="res://icon.png"

[autoload]

Simulador="*res://Scripts/Simulador.gd"
CPU="*res://Scripts/cpu.gd"
Memoria="*res://Scripts/memoria.gd"
Operacoes="*res://Scripts/Operacoes.gd"
Teste="*res://Scripts/Teste.gd"
Estado="*res://Scripts/Estado.gd"
Programa="*res://Scripts/Programa.gd"
UnidadeDeControle="*res://Scripts/UnidadeDeControle.gd"

[physics]

common/enable_pause_aware_picking=true

[rendering]

textures/canvas_textures/default_texture_repeat=1
renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
quality/driver/driver_name="GLES2"
vram_compression/import_etc=true
vram_compression/import_etc2=false
environment/default_environment="res://default_env.tres"
